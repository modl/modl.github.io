---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>MOD/L Articles</title>
    <link>http://modl.github.io/</link>
    <atom:link href="http://modl.github.io/rss.xml" rel="self" type="application/rss+xml" />
    <description>The latest articles from the Markit On Demand Labs team, MOD/L.</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://modl.github.io{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>{{post.author}}</author>
      <guid>http://modl.github.io{{ post.url }}</guid>
      <description>{{ post.excerpt | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel>
</rss>